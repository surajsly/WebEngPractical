<head>
	<title>Nested Loop</title>
</head>
<body>
	<h1 id="fab"></h1>
	<button onclick="Fibonacci()">Find Fibonacci</button>
</body>
<script type="text/javascript">
	function Fibonacci(){

		num = prompt("Enter the Number")
		num = parseInt(num)
		alert(num)
		var num1=0;
    	var num2=1;
    	var sum;
    	var i=0;
    	for (i = 0; i < num; i++) 
    	{
        	sum=num1+num2;
        	num1=num2;
        	num2=sum;
        	console.log(sum)
    	}
    	alert(sum)

	}
</script>
</html>